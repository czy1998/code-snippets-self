{
  "页面注释——详细": {
    "prefix": "hera-page-specific",
    "body": [
      "/**",
      " * @title ${1: 标题}",
      " * @pagePath ${2: 页面地址}",
      " * @router ${3: 路由}",
      " * @description ${4: 具体介绍}",
      " */"
    ],
    "description": "页面头部规范"
  },
  "页面注释——简易": {
    "prefix": "hera-page-simple",
    "body": [
      "/**",
      " * @title ${1: 标题}",
      " * @description ${2: 具体介绍}",
      " */"
    ],
    "description": "页面头部规范"
  },
  "组件注释": {
    "prefix": "hera-com",
    "body": [
      "/**",
      " * @title ${1: 标题}",
      " * @description ${2: 具体介绍}",
      " */"
    ],
    "description": "组件头部规范"
  },
  "函数注释——详细": {
    "prefix": "hera-func-specific",
    "body": [
      "/**",
      " * @title ${1: 标题}",
      " * @description ${2: 具体介绍}",
      " * @param ${3: 变量}",
      " * @return ${4: 返回结果}",
      " */"
    ],
    "description": "函数头部规范"
  },
  "函数注释——简易": {
    "prefix": "hera-func-specific",
    "body": [
      "/**",
      " * @title ${1: 标题}",
      " * @description ${2: 具体介绍}",
      " */"
    ],
    "description": "函数头部规范"
  },
  "获取当前时间": {
    "prefix": ["hera-time"],
    "body": [
      "/**",
      " * @完整时间 ${1: $CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE:$CURRENT_SECOND}",
      " * @月份 ${2: $CURRENT_MONTH_NAME_SHORT}",
      " * @星期 ${3: $CURRENT_DAY_NAME_SHORT}",
      " */"
    ],
    "description": "获取当前时间"
  },
  "获取当前文件信息": {
    "prefix": ["hera-fileInfo"],
    "body": [
      "/**",
      " * @文件名_带后缀 ${1: $TM_FILENAME}",
      " * @文件名_不带后缀 ${2: $TM_FILENAME_BASE}",
      " * @项目完整地址 ${3: $TM_DIRECTORY}",
      " * @文件完整地址 ${4: $TM_FILEPATH}",
      " * @文件相对地址 ${5: $RELATIVE_FILEPATH}",
      " */"
    ],
    "description": "获取当前文件信息"
  }
}
